// hotkeys

@install()
@inject(ADDRESS.HOTKEY_START)
fun hotkey_start() {
    subtext("Start");
}
@install()
@inject(ADDRESS.HOTKEY_START_L)
fun hotkey_start_l() {
    subtext("Start+L");

    if(True) {
        <0x2834> += 0x01;
    }
}
@install()
@inject(ADDRESS.HOTKEY_START_R)
fun hotkey_start_r() {
    subtext("Start+R");

    attribute(BOY, NO_CLIP, True);
    debug_boy();
}

