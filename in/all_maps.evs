            object[0x01] = 0x02; // change body (0=present, 1=cocoon, 2=gone)
            object[0x02] = 0x01; // change rip_entrance_right (0=present, 1=gone)
            object[0x03] = 0x01; // change rip_entrance_left (0=present, 1=gone)
        }
    }

