map thraxx(0x18) {
    enum entrance {
        north = entrance(0x18, 0x10, SOUTH),
        south = entrance(0x17, 0x3f, NORTH),
    }

    enum stepon_trigger {
        exit_north = @install() {
            subtext("S=0");

            map_transition(thraxx, south, NORTH);
        },
        trigger_boss = @install() {
            subtext("S=1");
        },
        exit_south = @install() {
            subtext("S=2");
            map_transition(thraxx, north, SOUTH);
        }
    }

    enum b_trigger {
        cocoon = @install() {
            subtext("B=0");
        },
        rip_cage = @install() {
            subtext("B=1");
        }
    }

    fun show_thraxx(show) {
        if(show) {
            add_enemy(ENEMY.THRAXX_LEFT_ARM, 0x13, 0x0e);
            add_enemy(ENEMY.THRAXX_RIGHT_ARM, 0x1d, 0x0d);

            object[0x00] = 0x02;
            object[0x02] = 0x01;
            object[0x03] = 0x02;
        } else {
            object[0x00] = 0x02;
            object[0x01] = 0x02;
            object[0x02] = 0x02;
            object[0x03] = 0x02;
        }
    }

    fun show_cocoon(show) {
        if(show) {
            object[0x01] = 0x01;
        } else {
            object[0x01] = 0x00;
        }
    }

    fun trigger_enter() {
        show_thraxx(False);
        // show_cocoon();

        fade_in();
    }
};

