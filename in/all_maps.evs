        <0x2834> += 0x01;
    }
}
@install()
@inject(ADDRESS.HOTKEY_START_R)
fun hotkey_start_r() {
    subtext("Start+R");

    attribute(BOY, NO_CLIP, True);
    debug_boy();
}

// helper

fun test_objects() {
    <0x2834> = 0x00;
    while(True) {
        <0x2836> = 0x00;
        while(<0x2836> < 0d3) {
            debug_memory(<0x2834>, <0x2836>);
            object[<0x2834>] = <0x2836>;
            sleep(0x50);
            <0x2836> += 0x01;
        }
        debug_memory(<0x2834>, 0x7e);
        object[<0x2834>] = 0x7e;
        sleep(0x50);
    }
}

