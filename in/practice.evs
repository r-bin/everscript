    add_enemy(ENEMY.PROFESSOR, 0x1e, 0x05);
    attach_script(CHARACTER.LAST_ENTITY, SCRIPT_TRIGGER.TALK, 0x1b72);
    entity_script_controlled(CHARACTER.LAST_ENTITY);

    fade_in();

    subtext(string("v1.2.0 - by r.bin - debug ring menu :)[END]"));
}

