            face(BOY, SOUTH);
            face(DOG, SOUTH);

            animate(ROOM.LIFT_TRIGGER, LOOP, ANIMATION_PLACEHOLDER.LIFT_MOVING);

            while(arg[0x00] < ROOM.LIFT_END) {
                ROOM.LIFT_TRIGGER[JUMP_HEIGHT] = arg[0x00];
                ROOM.LIFT_CORD[JUMP_HEIGHT] = arg[0x00];
