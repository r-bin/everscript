            arg[0x10] = randrange(0d05);

            if(arg[0x10] <= 0d00) {
                subtext("Bee uses tackle");
                damage(DOG, 0d05, True);

                BOSS.POKEMON[X] -= 0d10;
                sleep(0d10);
                BOSS.POKEMON[X] += 0d10;
