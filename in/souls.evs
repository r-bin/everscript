            ROOM.LIFT_CORD[FALLING_HEIGHT] = ROOM.LIFT_START;

            ROOM.LIFT_TRIGGER[FALLING_HEIGHT] = ROOM.LIFT_START;
            yield();
            animate(ROOM.LIFT_TRIGGER, LOOP, ANIMATION_PLACEHOLDER.LIFT_MOVING);

