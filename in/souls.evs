                    // sleep(0x30);

                    arg[0x04] = arg[0x00] & 0x01;
                    arg[0x06] = arg[0x02] & 0x01;

                    // debug_memory(arg[0x04], arg[0x06]);
                    // sleep(0x30);

                    arg[0x08] = arg[0x04] ^ arg[0x06];

                    // debug_memory(arg[0x08], arg[0x08]);
