                    MEMORY.STRING_PARAMETER_1 = ROOM.WELL_CRANKS;
                    MEMORY.STRING_PARAMETER_2 = ROOM.CRANKS_REQUIRED;
                    MEMORY.STRING_PARAMETER_3 = arg[0x10];

