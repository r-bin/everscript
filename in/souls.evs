                if(ROOM.LUIGI_ID > 0d0) {
                    arg[0x10] = ROOM.LUIGI_LIMIT_TOP + (ROOM.CRANKS_REQUIRED - ROOM.WELL_CRANKS);

                    if(arg[0x10] < ROOM.LUIGI_LIMIT_BOTTOM) {
