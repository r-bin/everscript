                    arg[0x08] = arg[0x04] - arg[0x00];
                } else {
                    arg[0x08] = arg[0x00] - arg[0x04];
                }

                if(arg[0x02] < arg[0x06]) {
                    arg[0x0a] = arg[0x06] - arg[0x02];
                } else {
                    arg[0x0a] = arg[0x02] - arg[0x06];
                }

                if((arg[0x08] <= 0x0a) && (arg[0x0a] <= 0x0a)) {
                    stone_hole_triggered();
                    end();
                }

                yield();

                if(ROOM.DEBUG) {
