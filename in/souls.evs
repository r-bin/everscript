                                face(MOUNT.ID, SOUTH);
                                walk(MOUNT.ID, COORDINATE_ABSOLUTE, MOUNT.ID[X], MOUNT.ID[Y] - MOUNT.WALK_DISTANCE, NONE);
                            } else if(<BOY>[FACE_DIRECTION] == FACE_DIRECTION.EAST) {
