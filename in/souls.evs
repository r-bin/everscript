            // puppet_1(0x40, 0x29);
            // puppet_1(0x6c, 0x29);
            // puppet_2(0x56, 0x30);


            puppet_1(0x4b, 0x29);
            attach_script(LAST_ENTITY, DAMAGE, reference(damage_1));
            BOSS.BOSS_1_ID = <LAST_ENTITY>;

            puppet_2(0x61, 0x29);
            attach_script(LAST_ENTITY, DAMAGE, reference(damage_2));
            BOSS.BOSS_2_ID = <LAST_ENTITY>;

