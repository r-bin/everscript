            fun double_hit_check() {
                while!(dead(BOSS.HEART_ID)) {
                    if((BOSS.HEART_ID[GENERAL_PURPOSE] > 0x00) && (BOSS.HEART_2_ID[GENERAL_PURPOSE] > 0x00)) {
