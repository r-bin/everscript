                claim_reward();

                if(ROOM.PROGRESS < ROOM.MAX_PROGRESS) {
                    question_max("[SLOW]Ready for the next oponent?[CHOICE]Yes…[CHOICE]No…[CHOICE]Leave…", 0d5);

