                PROGRESS = flag_tmp(),
                ID = memory_tmp(),

                HP = 0d100, // TODO: too low for >bonecrusher

                FOLLOWING = memory_tmp(),

                PUKE_HELPER = memory_tmp(),

                MOVE_RNG = memory_tmp(),

                CANNONBALL_X = memory_tmp(),
                CANNONBALL_Y = memory_tmp(),
                CANNONBALL_COUNT = memory_tmp(),
            }

            enum stepon_trigger {
                exit_east = @install() {
