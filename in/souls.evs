        fun prepare_dog() {
            control(DOG, True);

            attribute(DOG, AI_RUN, True);
            walk(DOG, TILE_ABSOLUTE, 0d26, 0d31, DOG); // TODO: unlocks BOTH
            face(DOG, EAST);
            sleep(0d30);
