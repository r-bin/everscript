                                walk(MOUNT.ID, COORDINATE_ABSOLUTE, MOUNT.ID[X] + MOUNT.WALK_DISTANCE, MOUNT.ID[Y], NONE);
                            } else if(<BOY>[FACE_DIRECTION] == FACE_DIRECTION.SOUTH) {
                                face(MOUNT.ID, NORTH);
                                walk(MOUNT.ID, COORDINATE_ABSOLUTE, MOUNT.ID[X], MOUNT.ID[Y] + MOUNT.WALK_DISTANCE, NONE);
                            } else if(<BOY>[FACE_DIRECTION] == FACE_DIRECTION.WEST) {
